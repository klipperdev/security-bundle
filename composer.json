{
    "name": "klipper/security-bundle",
    "type": "symfony-bundle",
    "description": "Security bundle of Klipper",
    "keywords": ["RBAC", "E-RBAC", "Permission", "Sharing", "Security"],
    "homepage": "https://github.com/klipperdev/security-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Fran√ßois Pluchino",
            "email": "francois.pluchino@klipper.dev"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4.0|^8.0.0",
        "symfony/framework-bundle": "^5.4.0",
        "symfony/security-bundle": "^5.4.0",
        "klipper/security": "^2.0.0"
    },
    "require-dev": {
        "symfony/form": "^5.4.0",
        "symfony/translation": "^5.4.0",
        "doctrine/doctrine-bundle": "^2.7.0",
        "doctrine/orm": "^2.13.0"
    },
    "autoload": {
        "psr-4": { "Klipper\\Bundle\\SecurityBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-main": "2.0-dev"
        }
    }
}
